{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      },
      "useBuiltIns": true,
      "modules": false
    }]
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-remove-strict-mode",
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "plugins": [
        "tcomb",
        "transform-es2015-modules-commonjs"
      ]
    },
    "development": {
      "plugins": [
        "tcomb"
      ]
    }
  }
}
